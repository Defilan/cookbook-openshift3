  <% if node['cookbook-openshift3']['openshift_master_asset_config'] %>
  <% for key in ['extensionScripts' , 'extensionStylesheets'] %>
  <% if node['cookbook-openshift3']['openshift_master_asset_config'].has_key?(key) %>
  <%= key %>:
  <%- node['cookbook-openshift3']['openshift_master_asset_config'][key].each do |name| -%>
    - <%= name %>
  <% end %>
  <% end %>
  <% end %>
  <% if node['cookbook-openshift3']['openshift_master_asset_config'].has_key?('extensions') %>
  extensions:
    - name: images
      sourceDirectory: <%= node['cookbook-openshift3']['openshift_master_asset_config']['extensions'] %>
  <% if @ose_major_version.split('.')[1].to_i >= 6 %>
  extensionScripts:
    - /etc/origin/master/openshift-ansible-catalog-console.js
  <% end %>
  <% end %>
  <% end %>
